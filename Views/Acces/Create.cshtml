@model LB.Models.Acces

@{
    ViewData["Title"] = "Create";
}

<h1>Nou</h1>

<h4>Acces LB</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="userWp" class="control-label">Usuari</label>
                <input asp-for="userWp" class="form-control" />
                <span asp-validation-for="userWp" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="passWd" class="control-label">Contrasenya</label>
                <input asp-for="passWd" class="form-control" type="password" />
                <span asp-validation-for="passWd" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="linkWp" class="control-label">Link</label>
                <input asp-for="linkWp" class="form-control" />
                <span asp-validation-for="linkWp" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="acc" class="control-label">Acces</label>
                <input asp-for="acc" class="form-control" />
                <span asp-validation-for="acc" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
